A.
#WAP to check if number is prime number
n=int(input("enter number to be checked:"))
def prime(n):
  flag=0
  for i in range(2,n):
    if n%i==0:
      flag=1
      break
  if flag==0:
    print("prime")
  else:
    print("not prime")
prime(n)
B.
# 2.k. wap to generate prime numbers till n
def isPrime(n):

    if(n==1 or n==0):  
        return False
    for i in range(2,n):
        if(n%i==0):
            return False
    return True
N = int(input("enter number till which prime numbers are to be generated:"))
for i in range(1,N+1):
  if(isPrime(i)):
        print(i)

C.

# 2.l. Generate first n prime numbers
def genrateprime(n):
    flag=False
    count=0
    i=2
    while(count<n):
        flag=True
        for j in range(2,i):
            if i%j==0:
                flag=False
                break
        if flag :
            print(i)
            count+=1
        i+=1
n=int(input("enter the amount of primes you want"))
genrateprime(n)
